    <% end %>

    <meta charset="utf-8">
    <title><%= content_for?(:title) ? yield(:title) : "Buy for your school" %></title>
    <meta name="viewport" content="width=device-width, initial-scale=1, viewport-fit=cover">
    <meta name="theme-color" content="#0b0c0c">

    <meta name="robots" content="noindex,nofollow">

    <link href="<%= image_path("images/favicon.svg") %>" rel="shortcut icon" type="image/x-icon">
    <link href="<%= image_path("images/govuk-icon-180.png") %>" rel="apple-touch-icon" sizes="180x180">
    <link href="<%= image_path("images/govuk-icon-192.png") %>" rel="apple-touch-icon" sizes="192x192">
    <link href="<%= image_path("images/govuk-icon-512.png") %>" rel="apple-touch-icon" sizes="512x512">
    <link href="<%= image_path("images/govuk-icon-mask.svg") %>" rel="mask-icon" color="#FFFFFF">

    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <meta property="og:image" content="<%=image_path("images/govuk-opengraph-image.png")%>">

    <%= stylesheet_link_tag "application" %>

    <% if support_portal? || frameworks_portal? %>
      <%= javascript_include_tag "tinymce/tinymce.min" %>
    <% end %>
    <%= javascript_include_tag "application" %>
  </head>

  <body class="govuk-template__body">

    <%= javascript_tag nonce: true do -%>
      document.body.className += ' js-enabled' + ('noModule' in HTMLScriptElement.prototype ? ' govuk-frontend-supported' : '');
    <% end -%>

    <% unless internal_portal? %>
      <%= render "cookie_preferences/banner" %>
    <% end %>

    <a href="#main-content" class="govuk-skip-link">Skip to main content</a>

    <header class="dfe-header" role="banner">
      <div class="dfe-header__container govuk-width-container">
        <div class="dfe-header__logo">
          <a class="dfe-header__link" href="/" aria-label="DfE homepage">
          <img src="/assets/images/dfe-logo.png" class="dfe-logo" alt="Department for Education">
          <img src="/assets/images/dfe-logo-alt.png" class="dfe-logo-hover" alt="Department for Education">
          </a>
        </div>
        <div class="dfe-header__content" id="content-header">
          <ul class="dfe-header__action-links">
            <% unless current_user.guest? %>
              <li id="userInfo">
                Signed in as <%= current_user.first_name %> <%= current_user.last_name %>
              </li>
              <li>
                <%= link_to I18n.t("generic.button.sign_out"), sign_out_path, method: :delete, class: "govuk-link govuk-link--inverse" %>
              </li>
            <% end %>
          </ul>
        </div>
      </div>
      <div class="govuk-width-container dfe-header__service-name">
        <a class="dfe-header__link--service" href="<%= header_link %>">
          <%= I18n.t("app.name") %>
        </a>
      </div>
      <%= render "layouts/navigation_links" %>
    </header>

    <div class="govuk-width-container">
      <div class="govuk-phase-banner">
        <p class="govuk-phase-banner__content">
          <strong class="govuk-tag govuk-phase-banner__content__tag">
            <%= banner_tag %>
          </strong>
          <span class="govuk-phase-banner__text">
            <%= banner_message.html_safe %>
          </span>
        </p>
      </div>
      <%= render partial: "layouts/breadcrumbs" %>
      <main class="govuk-main-wrapper " id="main-content">
        <%= render partial: "layouts/messages" %>
        <%= yield %>
      </main>
    </div>

    <footer class="govuk-footer">
      <div class="govuk-width-container">
        <div class="govuk-footer__meta">
          <div class="govuk-footer__meta-item govuk-footer__meta-item--grow">

            <ul class="govuk-footer__inline-list">
              <li class="govuk-footer__inline-list-item">
                <%= link_to "Accessibility statement", "/accessibility", class: "govuk-footer__link" %>
              </li>
              <li class="govuk-footer__inline-list-item">
                <%= link_to "Terms and Conditions", "/terms-and-conditions", class: "govuk-footer__link" %>
              </li>
              <li class="govuk-footer__inline-list-item">
                <%= link_to "Privacy notice", "/privacy", class: "govuk-footer__link" %>
              </li>
            </ul>

            <svg aria-hidden="true" focusable="false" class="govuk-footer__licence-logo" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 483.2 195.7" height="17" width="41">
              <path fill="currentColor" d="M421.5 142.8V.1l-50.7 32.3v161.1h112.4v-50.7zm-122.3-9.6A47.12 47.12 0 0 1 221 97.8c0-26 21.1-47.1 47.1-47.1 16.7 0 31.4 8.7 39.7 21.8l42.7-27.2A97.63 97.63 0 0 0 268.1 0c-36.5 0-68.3 20.1-85.1 49.7A98 98 0 0 0 97.8 0C43.9 0 0 43.9 0 97.8s43.9 97.8 97.8 97.8c36.5 0 68.3-20.1 85.1-49.7a97.76 97.76 0 0 0 149.6 25.4l19.4 22.2h3v-87.8h-80l24.3 27.5zM97.8 145c-26 0-47.1-21.1-47.1-47.1s21.1-47.1 47.1-47.1 47.2 21 47.2 47S123.8 145 97.8 145" />
            </svg>
            <span class="govuk-footer__licence-description">
              All content is available under the
              <a class="govuk-footer__link" href="https://www.nationalarchives.gov.uk/doc/open-government-licence/version/3/" rel="license">Open Government Licence v3.0</a>, except where otherwise stated
            </span>
          </div>
          <div class="govuk-footer__meta-item">
            <a class="govuk-footer__link govuk-footer__copyright-logo" href="https://www.nationalarchives.gov.uk/information-management/re-using-public-sector-information/uk-government-licensing-framework/crown-copyright/">© Crown copyright</a>
          </div>
        </div>
      </div>
    </footer>

    <%= render partial: "layouts/redirect" %>
  </body>

</html>
