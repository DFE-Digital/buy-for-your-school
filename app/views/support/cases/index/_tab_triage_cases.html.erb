<div class="govuk-tabs__panel govuk-tabs__panel--hidden" id="triage-cases">
  <h1 class="govuk-heading-l">
    <%= I18n.t("support.case.list.triage_cases") %>
  </h1>
  <%= render partial: "support/cases/case_list/case_list",
        locals: {
          tab: "triage-cases",
          cases: @triage_cases,
          page_param_name: "triage_cases_page",
          filter_form: @triage_cases_filter_form,
          filter_form_scope: :filter_triage_cases_form,
          filter_url: support_cases_path(anchor: "triage-cases"),
          clear_url: support_cases_path(anchor: "triage-cases", clear: true),
        } %>
</div>
