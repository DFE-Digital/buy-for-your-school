<div class="govuk-tabs__panel govuk-tabs__panel--hidden" id="new-cases">
  <h1 class="govuk-heading-l">
    <%= I18n.t("support.case.list.new_cases") %>
  </h1>
  <%= render partial: "support/cases/case_list/case_list",
        locals: {
          tab: "new-cases",
          cases: @new_cases,
          page_param_name: "new_cases_page",
          filter_form: @new_cases_filter_form,
          filter_form_scope: :filter_new_cases_form,
          filter_url: support_cases_path(anchor: "new-cases"),
          clear_url: support_cases_path(anchor: "new-cases", clear: true),
        } %>
</div>
