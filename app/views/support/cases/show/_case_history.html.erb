<div class="govuk-tabs__panel govuk-tabs__panel--hidden" id="case-history">
  <h2 class="govuk-heading-l">
    <%= I18n.t("support.case.header.case_history") %>
  </h2>

  <table class="govuk-table" id="case-history-table">
    <thead class="govuk-table__head">
      <tr class="govuk-table__row">
        <th class="govuk-table__header"><%= I18n.t("support.case.label.case_history.table.type") %></th>
        <th class="govuk-table__header"><%= I18n.t("support.case.label.case_history.table.note") %></th>
        <th class="govuk-table__header"><%= I18n.t("support.case.label.case_history.table.created") %></th>
      </tr>
    </thead>

    <tbody class="govuk-table__body">
      <%= render partial: "support/cases/case_history/interaction", collection: interactions, as: :interaction %>
    </tbody>
  </table>
</div>
