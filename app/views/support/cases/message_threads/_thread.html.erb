<tr class="govuk-table__row">
  <td class="govuk-table__cell">
    <strong>
      <%= thread.recipients %>
    </strong>
  </td>
  <td class="govuk-table__cell">
    <%= thread.subject %>
  </td>
  <td class="govuk-table__cell">
    <%= thread.last_updated %>
  </td>
  <td class="govuk-table__cell">
    <%= link_to I18n.t("support.case.label.message_threads.view"), support_case_message_thread_path(id: thread, back_to: Base64.encode64(@back_url)), class: "govuk-link" %>
  </td>
</tr>
