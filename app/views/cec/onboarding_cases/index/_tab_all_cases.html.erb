<div class="govuk-tabs__panel" id="all-cases">
  <h1 class="govuk-heading-l">
    <%= I18n.t("support.case.list.all_cases") %>
  </h1>
  <%= render partial: "cec/onboarding_cases/case_list/case_list",
      locals: {
        tab: "all-cases",
        cases: @all_cases,
        list_display_name: I18n.t("support.case.list.all_cases"),
        page_param_name: "all_cases_page",
        filter_form: @all_cases_filter_form,
        filter_form_scope: :filter_all_cases_form,
        filter_url: cec_onboarding_cases_path(anchor: "all-cases"),
        clear_url: cec_onboarding_cases_path(anchor: "all-cases", clear: true),
      } %>
</div>
