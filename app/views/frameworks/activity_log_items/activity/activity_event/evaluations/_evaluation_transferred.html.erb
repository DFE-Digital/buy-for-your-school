<% content_for :"#{activity.id}_subject" do %>
  Evaluation Transferred
<% end %>

<% content_for :"#{activity.id}_description" do %>
  <% unless activity.loaded_data.source_id.nil? %>
    <% kase = activity.loaded_data.source_type.safe_constantize.find(activity.loaded_data.source_id) %>
    Transferred from
    <%= link_to "Case #{kase.ref}", support_case_path(kase), class: "govuk-link govuk-link--no-visited-state", "target" => "_top" %>
  <% end %>
<% end %>
